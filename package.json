{
    "groupe": "test-cga",
    "name": "livingdoc",
    "version": "0.0.3",
    "description": "Projet de tests de génération de documentation",
    "private": true,
    "artifactory": {
        "docker": {
            "snapshot": "docker-snapshots.artefact-repo.pole-emploi.intra",
            "release": "docker-private.artefact-repo.pole-emploi.intra"
        }
    },
    "dependencies": {
        "gitlog": "^2.4.0",
        "viz.js": "^1.8.0"
    },
    "devDependencies": {
        "npm-incubateur-composant": "^0.1.6",
        "npm-incubateur-docker": "^0.2.13"
    },
    "scripts": {
        "nettoie": "mvn clean",
        "construit": "npm run nettoie && mvn package",
        "lance-dev": "npm run nettoie && mvn spring-boot:run",
        "genere-la-doc": "npm run nettoie && mvn process-resources && raml2html livingdoc.raml > src/doc/gen/livingdoc-api.html",
        "genere-les-releases-notes": "node scripts/release-notes.js"
    }
}
